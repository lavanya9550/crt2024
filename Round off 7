//Round off 7
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    float a;
    scanf("%f",&a);
    printf("%d\n",(int)a);
    printf("%.1f\n",ceil(a));
   printf("%.1f",floor(a));
    return 0;
}
