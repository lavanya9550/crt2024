#include <stdio.h>
#include <math.h>

int main()
{
    float a,b,c;
    scanf("%f%f%f",&a,&b,&c);
    float d=b*b-4*a*c;
    if(d<0)
    {
        printf("roots are not possible");
    }
    else
    {
        float r1=(-b+sqrt(b*b-4*a*c))/(2*a);
        float r2=(-b-sqrt(b*b-4*a*c))/(2*a);
        printf("Root1=%.1f\n",r1);
        printf("Root2=%.1f\n",r2);
    }
    
    return 0;
}
